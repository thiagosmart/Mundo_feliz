<div class="page-header">
  <%= link_to adminstradors_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_adminstrador_path(@adminstrador), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show adminstrador</h1>
</div>

<dl class="dl-horizontal">
  <dt>Nome:</dt>
  <dd><%= @adminstrador.nome %></dd>

  <dt>Email:</dt>
  <dd><%= @adminstrador.email %></dd>

  <dt>Senha:</dt>
  <dd><%= @adminstrador.senha %></dd>

</dl>
